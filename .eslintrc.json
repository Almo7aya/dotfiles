{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true,
    "es2022": true,
    "commonjs": true,
    "amd": true
  },
  "overrides": [
    {
      "files": [
        "*.ts"
      ],
      "parserOptions": {
        "project": [
          "tsconfig.json",
          "e2e/tsconfig.json"
        ],
        "createDefaultProgram": true
      },
      "extends": [
        "eslint:recommended"
      ],
      "rules": {
        "sort-imports": 0,
        "no-prototype-builtins": 0,
        "@typescript-eslint/no-unused-vars": 2,
        "@typescript-eslint/ban-types": 0,
        "@typescript-eslint/no-empty-function": 0,
        "no-empty": 0,
        "no-case-declarations": 0,
        "@typescript-eslint/no-var-requires": 0,
        "no-useless-escape": 0,
        "no-control-regex": 0
      }
    }
  ]
}
